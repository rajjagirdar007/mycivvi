<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <title>MPENGS Admin</title>


    <link rel="stylesheet" href="/css/bootstrap/bootstrap.min.css">    

    <!-- Favicons -->
<link rel="apple-touch-icon" href="/docs/5.1/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
<link rel="icon" href="/docs/5.1/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
<link rel="icon" href="/docs/5.1/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
<link rel="manifest" href="/docs/5.1/assets/img/favicons/manifest.json">
<link rel="mask-icon" href="/docs/5.1/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
<link rel="icon" href="/docs/5.1/assets/img/favicons/favicon.ico">
<meta name="theme-color" content="#7952b3">


    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }
    </style>

    
    <!-- Custom styles for this template -->
    <link href="/css/admin.css" rel="stylesheet">
  </head>
  <body>
    
<header class="navbar navbar-dark sticky-top bg-dark flex-md-nowrap p-0 shadow">
  <a class="navbar-brand col-md-3 col-lg-2 me-0 px-3" href="#">MPENGS</a>
  <button class="navbar-toggler position-absolute d-md-none collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="navbar-nav">
    <div class="nav-item text-nowrap">
      <a class="nav-link px-3" href="/logout">Logout</a>
    </div>
  </div>
</header>

<div class="container-fluid">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link" href="/administrator/users">
              <span data-feather="users"></span>
              Users
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/administrator/feedback">
              <span data-feather="bar-chart-2"></span>
              Feedback
            </a>
          </li>
        </ul>

        <ul class="nav flex-column mb-2">
          <li class="nav-item">
            <a class="nav-link" href="/administrator/leaderboard">
              <span data-feather="file-text"></span>
              Leaderboards
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/vue12#/1">
              <span data-feather="file-text"></span>
              Inventory
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/administrator/projects_panel">
              <span data-feather="file-text"></span>
	      Projects Panel
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/administrator/viewcareer">
              <span data-feather="file-text"></span>
	      Career Panel
            </a>
          </li>
	</ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
	      <h1 class="h2">Admin {{username}}</h1>
	      <p> </p>
        <div class="btn-toolbar mb-2 mb-md-0">
        </div>
      </div>

      <a href="/administrator/viewcareer" class="btn btn-secondary">Back to Careers</a>
      <p> </p>
    <form action="/administrator/edit/careeredit/{{info.id}}" method="POST">
	    <div class="form-group">
		    <label for="jtitle">Job Title</label>
		    <input id="jtitle" type="text" class="form-control" placeholder="Job Title" name="title" value="{{info.title}}" required>
	    </div>
	    <div class="form-group">
		  <label for="jloc">Location</label>
		  <input id="jloc" class="form-control" type="text" placeholder="Location of Job" name="location" value="{{info.location}}" required>
	    </div>
	    <div class="form-group">
		  <label for="jexp">Experience</label>
		  <input id="jexp" class="form-control" type="text" placeholder="Industry Related Experience" name="experience" value="{{info.experience}}" required>
	    </div>
	    <div class="form-group">
		  <label for="jskills">Key Skills</label>
		  <input id="jskills" class="form-control" type="text" placeholder="Industry Related Skills" name="key_skills" value="{{info.key_skills}}" required>
	    </div>
	    <div class="form-group">
		  <label for="jdesc">Description</label>
		  <textarea id="jdesc" class="form-control" placeholder="Position Description" name="desc"required>{{info.desc}}</textarea>
		  <p> </p> 
	  <button type="submit" class="btn btn-primary">Submit</button>
    </form>

	

    </main>
	<script src = "/js/bootstrap/bootstrap.bundle.min.js"></script>
  </body>
</html>

















